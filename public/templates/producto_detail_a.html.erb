<li class="producto">
	<div class="imagen">
		<img src="http://btc.cr<%= item.attr('imagen') %>"/>
	</div>
	<div class="details">
	
	<h2><%= item.attr('titulo') %></h2>
	<h4><%= item.attr('description') %></h4>
	
	<ul>
		<%= item.attr('especs') %>
	</ul>
	<h3><%= item.attr('precio') %></h3>
	</div>
</li>
